{
    "name": "mc-discord-auth",
    "version": "1.0.0",
    "description": "Discord bot for Minecraft server authentication running on Vercel",
    "scripts": {
        "dev": "vercel dev",
        "build": "tsc",
        "test": "vitest",
        "test:ci": "vitest run",
        "deploy": "vercel --prod",
        "register-commands": "tsx scripts/register-commands.ts register",
        "list-commands": "tsx scripts/register-commands.ts list",
        "delete-commands": "tsx scripts/register-commands.ts delete",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db:migrate": "prisma migrate dev",
        "db:studio": "prisma studio",
        "db:reset": "prisma migrate reset"
    },
    "dependencies": {
        "@prisma/client": "^6.15.0",
        "@vercel/node": "^5.3.16",
        "discord-interactions": "^4.3.0",
        "tweetnacl": "^1.0.3",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@types/node": "^24.3.0",
        "@types/uuid": "^10.0.0",
        "prisma": "^6.15.0",
        "tsx": "^4.20.5",
        "typescript": "^5.9.2",
        "vercel": "^46.0.5",
        "vitest": "^3.2.4"
    },
    "engines": {
        "node": ">=22"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "@prisma/client",
            "@prisma/engines",
            "esbuild",
            "prisma"
        ]
    },
    "license": "MIT"
}